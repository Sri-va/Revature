<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="" packages="">
  <Appenders>
    
      <File name="MyFile" fileName="C:\\logs\\app.log">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
      </File>
     
  </Appenders>
  <Loggers>
    <Root level="trace">
      
      <AppenderRef ref="MyFile"/>
    </Root>
  </Loggers>
</Configuration>